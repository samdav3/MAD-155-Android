<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/mainActivity">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.example.davcalbeansapp.ui.home.HomeFragment"
        android:label="@string/menu_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/nav_gallery"
        android:name="com.example.davcalbeansapp.ui.gallery.GalleryFragment"
        android:label="@string/menu_gallery"
        tools:layout="@layout/fragment_gallery" />

    <fragment
        android:id="@+id/nav_slideshow"
        android:name="com.example.davcalbeansapp.ui.slideshow.SlideshowFragment"
        android:label="@string/menu_slideshow"
        tools:layout="@layout/fragment_slideshow" />
    <activity
        android:id="@+id/mainActivity"
        android:name="com.example.davcalbeansapp.MainActivity"
        android:label="@string/menu_home"
        tools:layout="@layout/activity_main" />
    <fragment
        android:id="@+id/beansFragment"
        android:name="com.example.davcalbeansapp.menu.BeansFragment"
        android:label="fragment_beans"
        tools:layout="@layout/fragment_beans" />
    <fragment
        android:id="@+id/createOwnFragment"
        android:name="com.example.davcalbeansapp.menu.CreateOwnFragment"
        android:label="fragment_create_own"
        tools:layout="@layout/fragment_create_own" />
    <fragment
        android:id="@+id/preMadeFragment"
        android:name="com.example.davcalbeansapp.menu.PreMadeFragment"
        android:label="fragment_pre_made"
        tools:layout="@layout/fragment_pre_made" />
    <fragment
        android:id="@+id/pageOneOrderFragment"
        android:name="com.example.davcalbeansapp.order.PageOneOrderFragment"
        android:label="fragment_page_one_order"
        tools:layout="@layout/fragment_page_one_order" />
    <fragment
        android:id="@+id/pageTwoOrderFragment"
        android:name="com.example.davcalbeansapp.order.PageTwoOrderFragment"
        android:label="fragment_page_two_order"
        tools:layout="@layout/fragment_page_two_order" />
    <activity
        android:id="@+id/account"
        android:name="com.example.davcalbeansapp.account.Account"
        android:label="activity_account"
        tools:layout="@layout/activity_account" />
    <activity
        android:id="@+id/contactUs"
        android:name="com.example.davcalbeansapp.contact.ContactUs"
        android:label="activity_contact_us"
        tools:layout="@layout/activity_contact_us" />
    <activity
        android:id="@+id/accountOrders"
        android:name="com.example.davcalbeansapp.account.AccountOrders"
        android:label="activity_account_orders"
        tools:layout="@layout/activity_account_orders" />
    <activity
        android:id="@+id/menu"
        android:name="com.example.davcalbeansapp.menu.Menu"
        android:label="activity_menu"
        tools:layout="@layout/activity_menu" />
</navigation>